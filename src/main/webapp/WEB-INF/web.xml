<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>channel</servlet-name>
		<servlet-class>htnn.ChannelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>channel</servlet-name>
		<url-pattern>/channel</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>cron</servlet-name>
		<servlet-class>htnn.CronServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cron</servlet-name>
		<url-pattern>/cron</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>publish</servlet-name>
		<servlet-class>htnn.PublishServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>publish</servlet-name>
		<url-pattern>/publish</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>sign</servlet-name>
		<servlet-class>htnn.SignGuestbookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sign</servlet-name>
		<url-pattern>/sign</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
